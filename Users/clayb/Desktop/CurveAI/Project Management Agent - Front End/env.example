# Anthropic API Configuration (for MCP)
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# n8n MCP Server Configuration
VITE_N8N_SSE_URL=https://your-n8n-instance.com/webhook/sse-endpoint
VITE_N8N_AUTH_TOKEN=your_n8n_auth_token_here

# Gemini API Configuration (fallback or alternative)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Feature Flags
# Set to 'true' to use MCP with Anthropic and n8n
VITE_USE_MCP=true

# Set to 'true' to use Gemini (will be used as fallback if MCP is enabled)
VITE_USE_GEMINI=false 